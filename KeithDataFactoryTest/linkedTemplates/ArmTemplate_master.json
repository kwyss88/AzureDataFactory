{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "KeithDataFactoryTest"
		},
		"DataFactory_Sql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DataFactory_Sql'"
		},
		"HcmsKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://wyssvault.vault.azure.net/"
		},
		"Hcms_Mongo_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "store--mongoDb--configuration"
		},
		"Hcms_Mongo_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Squidex"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataFactory_Sql_connectionString": {
						"value": "[parameters('DataFactory_Sql_connectionString')]"
					},
					"HcmsKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('HcmsKeyVault_properties_typeProperties_baseUrl')]"
					},
					"Hcms_Mongo_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('Hcms_Mongo_properties_typeProperties_connectionString_secretName')]"
					},
					"Hcms_Mongo_properties_typeProperties_database": {
						"value": "[parameters('Hcms_Mongo_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}